doctype html
html
  head
    meta(charset='UTF-8')
    | 
    link(rel='stylesheet', type='text/css', href='/css/classes-style.css')
    | 
    link(href='http://fonts.googleapis.com/icon?family=Material+Icons', rel='stylesheet')
    | 
    link(type='text/css', rel='stylesheet', href='/css/materialize.css', media='screen,projection')
    | 
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    | 
    script(type='text/javascript', src='https://code.jquery.com/jquery-2.1.1.min.js')
    |       
    script(type='text/javascript', src='/js/materialize.js')
  
    script(type='text/javascript', src='/js/classes.js')

    script(src='/socket.io/socket.io.js')
    
     
  
  body
    header
      nav.z-depth-1
        .nav-wrapper.white
          a.brand-logo.center.black-text(href='/') DCSReGQ
          ul#nav-mobile.left.hide-on-med-and-down
            li
              a.black-text(href='/login') login
            | 
            li
              a.black-text(href='/classes') classes
            | 
            li
              a.black-text(href='#') about
    | 
    main
      .row.center-align.white-text
        p
          span Availablity as of: #{updateTime}
        p
          a(id='#update-btn') UPDATE NOW
      .row
        each slots,section in classList
          .col.s12.m6.l4
              .card.blue-grey.darken-1
                .card-content.white-text
                  span.card-title #{section}        
                  p Available slots: #{slots}
                    
                |       
                //.card-action
                  a(href='#') This is a link



