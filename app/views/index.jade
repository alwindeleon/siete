    script.
      $(document).ready(function(){
      $('.slk').slick({
      dots: true,
      autoplay: true,
      autoplaySpeed: 3000,
      slidesToShow: 4,
      infinite: true,
      arrows: true,
      mobileFirst: true,
      responsive: [
      {
      breakpoint: 1024,
      settings: {
      slidesToShow: 4,
      slidesToScroll: 1,
      infinite: true,
      dots: true
      }
      },
      {
      breakpoint: 600,
      settings: {
      slidesToShow: 3,
      slidesToScroll: 1
      }
      },
      {
      breakpoint: 480,
      settings: {
      slidesToShow: 1,
      slidesToScroll: 1
      }
      },
      {
      breakpoint: 320,
      settings: {
      slidesToShow: 1,
      slidesToScroll: 1
      }
      },
      {
      breakpoint: 240,
      settings: {
      slidesToShow: 1,
      slidesToScroll: 1
      }
      }
      ],
      });
      });
