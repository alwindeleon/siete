doctype html
html
  head
    meta(charset='UTF-8')
    | 
    link(rel='stylesheet', type='text/css', href='/css/slick.css')
    | 
    link(rel='stylesheet', type='text/css', href='/css/slick-theme.css')
    | 
    link(rel='stylesheet', type='text/css', href='/css/mat-style.css')
    | 
    link(href='http://fonts.googleapis.com/icon?family=Material+Icons', rel='stylesheet')
    | 
    link(type='text/css', rel='stylesheet', href='/css/materialize.css', media='screen,projection')
    | 
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    | 
    script(type='text/javascript', src='https://code.jquery.com/jquery-2.1.1.min.js')
    |       
    script(type='text/javascript', src='/js/materialize.js')
    |       
    script(type='text/javascript', src='/js/slick.js')
    |       
    script.
      $(document).ready(function(){
      $('.slk').slick({
      dots: true,
      autoplay: true,
      autoplaySpeed: 3000,
      slidesToShow: 4,
      infinite: true,
      arrows: true,
      mobileFirst: true,
      responsive: [
      {
      breakpoint: 1024,
      settings: {
      slidesToShow: 4,
      slidesToScroll: 1,
      infinite: true,
      dots: true
      }
      },
      {
      breakpoint: 600,
      settings: {
      slidesToShow: 3,
      slidesToScroll: 1
      }
      },
      {
      breakpoint: 480,
      settings: {
      slidesToShow: 1,
      slidesToScroll: 1
      }
      },
      {
      breakpoint: 320,
      settings: {
      slidesToShow: 1,
      slidesToScroll: 1
      }
      },
      {
      breakpoint: 240,
      settings: {
      slidesToShow: 1,
      slidesToScroll: 1
      }
      }
      ],
      });
      });
  | 
  body
    header
      nav.z-depth-1
        .nav-wrapper.white
          a.brand-logo.center.black-text(href='/') DCSReGQ
          | 
          | 
          ul#nav-mobile.left.hide-on-med-and-down
            li
              a.black-text(href='/login') login
            | 
            li
              a.black-text(href='#') classes
            | 
            li
              a.black-text(href='#') about
    | 
    main
      .row
        .col.s12.l12
          #queue-carousel.slk
            if Object.keys(queuesAdvise).length > 0
              each val,key in queuesAdvise
                .col.l2.s12.center-align
                  .card-panel.batch1.headPadding
                    strong
                      h5 #{key}
                  | 
                  .card-panel.collection
                    each item in queuesAdvise[key]
                      a.collection-item.batch1(href='#!') #{item}
            if Object.keys(queuesEnlist).length > 0  
              each val,key in queuesEnlist
                .col.l2.s12.center-align
                  .card-panel.batch1.headPadding
                    strong
                      h5 #{key}
                  | 
                  .card-panel.collection
                    each item in queuesEnlist[key]
                      a.collection-item.batch1(href='#!') #{item}

                     
                    
            
            
           
